          return 'auth_state必须是数字';
        }
        var reg = /^[0-9]+$/;
        if(!reg.test(value.trim())) {
          return 'auth_state必须是整数';
        }
      },
      identity_state: function (value, item) {
        if (!value.trim()) {
          return 'identity_state不能为空';
        }
        var reg = /^(\-|\+)?\d+(\.\d+)?$/;
        if(!reg.test(value.trim())) {
          return 'identity_state必须是数字';
        }
        var reg = /^[0-9]+$/;
        if(!reg.test(value.trim())) {
          return 'identity_state必须是整数';
        }
      },
      education_state: function (value, item) {
        if (!value.trim()) {
          return 'education_state不能为空';
        }
        var reg = /^(\-|\+)?\d+(\.\d+)?$/;
        if(!reg.test(value.trim())) {
