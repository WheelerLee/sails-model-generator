<script>
  layui.use(['form', 'laydate'], function () {
    var $ = layui.$, layer = layui.layer, form = layui.form;

    /* 自定义验证规则 */
    form.verify({
      nick_name: function (value, item) {
        if (value.trim().length > 200) {
          return 'nick_name的长度必须小于200';
        }
      },
      head_icon: function (value, item) {
        if (value.trim().length > 32) {
          return 'head_icon的长度必须小于32';
        }
      },
      sur_name: function (value, item) {
        if (value.trim().length > 100) {
          return 'sur_name的长度必须小于100';
        }
      },
      name: function (value, item) {
        if (value.trim().length > 100) {
          return 'name的长度必须小于100';
        }
      },
      vacation_mode: function (value, item) {
        if (!value.trim()) {
          return 'vacation_mode不能为空';
        }
        var reg = /^(\-|\+)?\d+(\.\d+)?$/;
        if(!reg.test(value.trim())) {
          return 'vacation_mode必须是数字';
        }
        var reg = /^[0-9]+$/;
        if(!reg.test(value.trim())) {
          return 'vacation_mode必须是整数';
        }
      },
      type: function (value, item) {
        if (!value.trim()) {
          return 'type不能为空';
        }
        var reg = /^(\-|\+)?\d+(\.\d+)?$/;
        if(!reg.test(value.trim())) {
          return 'type必须是数字';
